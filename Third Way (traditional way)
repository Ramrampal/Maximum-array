#include<iostream>
using namespace std;
int main(){
    int arr[]={4,5,6,34,65,67,78,3,54,6};
    int n= sizeof(arr)/sizeof(arr[0]);
    int mx= INT16_MIN; // Store minimun value of intiger ðŸ‘Œ
    for(int i=0;i<n;i++){
        // if(max<arr[i]){
        //     max=arr[i];
        // }
        mx = max(mx,arr[i]);
    }
    cout<<mx;


}
